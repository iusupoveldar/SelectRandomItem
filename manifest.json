{
    "manifest_version":3,
    "name":"Select Random Item In Steam Inventory",
    "version":"1.0",
    "permissions":[
        "*://steamcommunity.com/*"
    ],
    "content_scripts":[
        {
            "matches":[
                "*://steamcommunity.com/*/inventory",
                "*://steamcommunity.com/*/inventory/*"
            ],
            "run_at":"document_idle",
            "js":["init.js"]
        }
    ]
}